    "core-js": "^3.3.2",
    "v-calendar": "^1.0.0-beta.23",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-select": "^3.4.0"
  },
  "author": "Sequis Frontend Dev",
  "main": "./dist/superyou-components.common.js",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-notes": "^5.2.8",
